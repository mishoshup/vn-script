{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "vn_script",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "statement"
      }
    },
    "statement": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "say"
        },
        {
          "type": "SYMBOL",
          "name": "narrate"
        },
        {
          "type": "SYMBOL",
          "name": "jump"
        },
        {
          "type": "SYMBOL",
          "name": "label"
        },
        {
          "type": "SYMBOL",
          "name": "end"
        },
        {
          "type": "SYMBOL",
          "name": "bg"
        },
        {
          "type": "SYMBOL",
          "name": "char"
        },
        {
          "type": "SYMBOL",
          "name": "menu"
        }
      ]
    },
    "say": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "say"
        },
        {
          "type": "FIELD",
          "name": "speaker",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "FIELD",
          "name": "text",
          "content": {
            "type": "SYMBOL",
            "name": "string"
          }
        }
      ]
    },
    "narrate": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "narrate"
        },
        {
          "type": "FIELD",
          "name": "text",
          "content": {
            "type": "SYMBOL",
            "name": "string"
          }
        }
      ]
    },
    "jump": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "jump"
        },
        {
          "type": "FIELD",
          "name": "target",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        }
      ]
    },
    "label": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "label"
        },
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        }
      ]
    },
    "end": {
      "type": "STRING",
      "value": "end"
    },
    "bg": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "bg"
        },
        {
          "type": "FIELD",
          "name": "file",
          "content": {
            "type": "SYMBOL",
            "name": "string"
          }
        }
      ]
    },
    "char": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "char"
        },
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "FIELD",
          "name": "pose",
          "content": {
            "type": "SYMBOL",
            "name": "filename"
          }
        }
      ]
    },
    "menu": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "menu"
        },
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "SYMBOL",
            "name": "choice"
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "choice": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "text",
          "content": {
            "type": "SYMBOL",
            "name": "string"
          }
        },
        {
          "type": "STRING",
          "value": "->"
        },
        {
          "type": "FIELD",
          "name": "target",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        }
      ]
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-zA-Z_][a-zA-Z0-9_]*"
    },
    "filename": {
      "type": "PATTERN",
      "value": "[a-zA-Z_][a-zA-Z0-9_.]+"
    },
    "string": {
      "type": "PATTERN",
      "value": "\"[^\"]*\""
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "#"
          },
          {
            "type": "PATTERN",
            "value": ".*"
          }
        ]
      }
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    },
    {
      "type": "SYMBOL",
      "name": "comment"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}